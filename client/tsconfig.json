{
    "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
    "compilerOptions": {
        "baseUrl": ".",
        "importsNotUsedAsValues": "preserve",
        "preserveValueImports": false,
        "noImplicitAny": false,
        "isolatedModules": false,
        "types": ["node","global.d.ts","element-plus/global"],
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
